<%
include('/_utils');

const params = ejsUtils.defaultParamsMapper(locals, {
  layer: null,
  title: null,
  dirname: true,
  un_class: null,
  un_id: null,
  un_canonical: null
});

const common = {
  title: 'TEST',
  description: '●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●',
  keywords: '●●●,●●●,●●●,●●●,●●●',
  canonical: '●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●',
  number: '●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●',
};
%>

<!doctype html>
<html>

<head>
  <!-- meta start -->
  <meta charset="utf-8">
  <% if (params.title) { %>
  <title><%- params.title %>｜<%- common.title %> </title>
  <% } else { %>
  <title><%- common.title %><%- params.layer %></title>
  <% } %>
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="format-detection" content="telephone=no">
  <meta name="description" content="<%- common.description %>">
  <meta name="keywords" content="<%- common.keywords %>">
  <meta property="og:type" content="article">
  <meta property="og:title" content="<%- common.title %>">
  <meta property="og:url" content="<%- common.canonical %>">
  <meta property="og:image" content="<%- common.number %>.jpg">
  <meta property="og:description" content="<%- common.description %>">
  <meta property="og:site_name" content="●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●●">
  <% if (params.un_canonical === "") { %>
  <link rel="canonical" href="<%- common.canonical %>">
  <% } else { %>
  <link rel="canonical" href="<%- common.canonical %><%- params.un_canonical %>">
  <% } %>
  <link rel="shortcut icon" href="/favicon.ico">
  <!-- meta end -->
  <!-- common css start -->
  <!-- common css end -->
  <!-- custom css start -->
  <link href="<%- params.layer %>assets/css/global.css" rel="stylesheet" type="text/css" media="all">
  <link rel="stylesheet" type="text/css" href="<%- params.layer %>assets/js/vendor/aos/aos.css" />
  <link rel="stylesheet" type="text/css" href="<%- params.layer %>assets/js/vendor/swiper/swiper.min.css" />
  <link rel="stylesheet" type="text/css" href="<%- params.layer %>assets/css/common.css">
  <link rel="stylesheet" type="text/css" href="./css/style.css">

  <!-- custom css end -->
  <!-- common sciript start -->
  <script type="text/javascript" src="<%- params.layer %>assets/js/vendor/jquery.js"></script>
  <script type="text/javascript" src="<%- params.layer %>assets/js/vendor/jquery.easing.1.3.js"></script>
  <script type="text/javascript" src="<%- params.layer %>assets/js/vendor/rellax/rellax.min.js"></script>
  <script type="text/javascript" src="<%- params.layer %>assets/js/vendor/swiper/swiper.min.js"></script>
  <!-- common sciript end -->
  <!-- local js start -->
  <script type="text/javascript" src="<%- params.layer %>assets/js/vendor/jquery.mousewheel.js" id="mousewheelJS" charset="UTF-8"></script>
  <script type="text/javascript" src="<%- params.layer %>assets/js/app.js" id="commonJS" charset="UTF-8"></script>

</head>

<body class="l-property-temp js-fadeAnime is-fadeout" data-code="01230">

  <div class="wrapper" id="<%- params.un_id %>">

    <!-- //////////////////////////////////////////////////////////////////////////////////////////////////// -->

    <div id="container">
      <div class="allContentWrap<% if (!(params.un_id === 'index')) { %> js-leafAnimeZone<% } %>">
